{
  "name": "darkord-sls-template",
  "version": "1.0.0",
  "description": "Serverless template that contains common things to be imported into new microservices of Darkord",
  "scripts": {
    "deploy:stage": "cross-env AWS_SDK_LOAD_CONFIG=1 sls deploy --stage stage --verbose",
    "deploy:prod": "cross-env AWS_SDK_LOAD_CONFIG=1 sls deploy --stage prod --verbose --conceal",
    "deploy:api": "cross-env AWS_SDK_LOAD_CONFIG=1 sls module-api:deploy --stage stage --verbose",
    "deploy:api:function": "cross-env AWS_SDK_LOAD_CONFIG=1 sls module-api:deploy function --stage stage --verbose -f",
    "deploy:db": "cross-env AWS_SDK_LOAD_CONFIG=1 sls module-db:deploy --stage stage --verbose",
    "deploy:funcs": "cross-env AWS_SDK_LOAD_CONFIG=1 sls module-funcs:deploy --stage stage --verbose",
    "deploy:funcs:function": "cross-env AWS_SDK_LOAD_CONFIG=1 sls module-funcs:deploy function --stage stage --verbose -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ii887522/darkord-sls-template.git"
  },
  "author": "ii887522",
  "bugs": {
    "url": "https://github.com/ii887522/darkord-sls-template/issues"
  },
  "homepage": "https://github.com/ii887522/darkord-sls-template#readme",
  "devDependencies": {
    "@serverless/compose": "^1.3.0",
    "cross-env": "^7.0.3",
    "serverless": "^3.39.0",
    "serverless-better-credentials": "^2.0.0",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-latest-layer-version": "^2.1.2",
    "serverless-prune-plugin": "^2.0.2",
    "serverless-rust-plugin": "^0.3.3"
  }
}
